{
  "_comment": "Simple boot test on panda",
  "job_name": "panda-boot",
  "device_type": "panda-es",
  "timeout": 1800,
  "actions": [
    {
      "command": "deploy_linaro_image",
      "parameters":
        {
          "image": "CREATE A TEST IMAGE.gz"
        }
    },
    {
      "command": "boot_linaro_image"
    },
    {
      "command": "submit_results",
      "parameters":
        {
          "server": "LAVA_RPC_LOGIN",
          "stream": "BUNDLE_STREAM"
        }
    }
  ]
}
